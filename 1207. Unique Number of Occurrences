class Solution:
    def uniqueOccurrences(self, arr: List[int]) -> bool:
        d = {}
        for i in range(len(arr)):
            if arr[i] in d:
                d.update({arr[i]:d[arr[i]]+1})
            else:
                d.update({arr[i]:1})
        v = sorted(d.values())
        print(v)
        for z in range(len(v)-1):
            if v[z] == v[z + 1]:
                return False
        return True
